<?xml version="1.0" encoding="UTF-8"?>
<project name="module_adpautoconnectproject" default="compile.module.adpautoconnectproject">
  <dirname property="module.adpautoconnectproject.basedir" file="${ant.file.module_adpautoconnectproject}"/>
  
  <property name="module.jdk.home.adpautoconnectproject" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.adpautoconnectproject" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.adpautoconnectproject" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.adpautoconnectproject" value="${compiler.args}"/>
  
  <property name="adpautoconnectproject.output.dir" value="${module.adpautoconnectproject.basedir}/build/classes/production/ADPAutoConnectProject"/>
  <property name="adpautoconnectproject.testoutput.dir" value="${module.adpautoconnectproject.basedir}/build/classes/test/ADPAutoConnectProject"/>
  
  <path id="adpautoconnectproject.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="adpautoconnectproject.module.production.classpath">
    <path refid="${module.jdk.classpath.adpautoconnectproject}"/>
  </path>
  
  <path id="adpautoconnectproject.runtime.production.module.classpath">
    <pathelement location="${adpautoconnectproject.output.dir}"/>
  </path>
  
  <path id="adpautoconnectproject.module.classpath">
    <path refid="${module.jdk.classpath.adpautoconnectproject}"/>
    <pathelement location="${adpautoconnectproject.output.dir}"/>
  </path>
  
  <path id="adpautoconnectproject.runtime.module.classpath">
    <pathelement location="${adpautoconnectproject.testoutput.dir}"/>
    <pathelement location="${adpautoconnectproject.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.adpautoconnectproject">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.adpautoconnectproject">
    <patternset refid="excluded.from.module.adpautoconnectproject"/>
  </patternset>
  
  
  <target name="compile.module.adpautoconnectproject" depends="compile.module.adpautoconnectproject.production,compile.module.adpautoconnectproject.tests" description="Compile module ADPAutoConnectProject"/>
  
  <target name="compile.module.adpautoconnectproject.production" depends="register.custom.compilers" description="Compile module ADPAutoConnectProject; production classes"/>
  
  <target name="compile.module.adpautoconnectproject.tests" depends="register.custom.compilers,compile.module.adpautoconnectproject.production" description="compile module ADPAutoConnectProject; test classes" unless="skip.tests"/>
  
  <target name="clean.module.adpautoconnectproject" description="cleanup module">
    <delete dir="${adpautoconnectproject.output.dir}"/>
    <delete dir="${adpautoconnectproject.testoutput.dir}"/>
  </target>
</project>